<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "org.mehaexample.asdDemo.model.PriorEducation" table = "PriorEducation">
      
      <meta attribute = "class-description">
         This class contains the Student's Prior Education Information. 
      </meta>
      
      <id name = "id" type = "int" column = "id">
         <generator class="native"/>
      </id>
      
      <property name = "nuid" column = "nuid" type = "string"/>    
      <property name = "InstitutionName" column = "institution_name" type = "string"/>
      <property name = "DegreeLevel" column = "degree_level" type = "string"/>
      <property name = "Major" column = "major" type = "string"/>
     
   </class>
</hibernate-mapping>