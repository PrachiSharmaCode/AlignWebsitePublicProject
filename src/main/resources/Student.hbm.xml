<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "org.mehaexample.asdDemo.model.Student" table = "STUDENT">
      
      <meta attribute = "class-description">
         This class contains the Student detail. 
      </meta>
      
      <id name = "id" type = "int" column = "id">
         <generator class="native"/>
      </id>
      
      <property name = "neuId" column = "NeuId" type = "string"/>    
      <property name = "email" column = "Email" type = "string"/>
      <property name = "firstName" column = "FirstName" type = "string"/>
      <property name = "middleName" column = "MiddleName" type = "string"/>
      <property name = "lastName" column = "LastName" type = "string"/>
      <property name = "gender" column = "Gender" type = "string"/>
      <property name = "phone" column = "Phone" type = "string"/>
      <property name = "address" column = "Address" type = "string"/>
      <property name = "state" column = "State" type = "string"/>
      <property name = "zip" column = "Zip" type = "string"/>
      <property name = "enrollmentStatus" column = "EnrollmentStatus" type = "string"/>
      <property name = "campus" column = "Campus" type = "string"/>
      <property name = "degreeCandidacy" column = "DegreeCandidacy" type = "string"/>
      <property name = "photo" column = "Photo" type = "string"/>
      
   </class>
</hibernate-mapping>