<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "org.mehaexample.asdDemo.model.PriorEducations" table = "PriorEducations">
      
      <meta attribute = "class-description">
         This class contains the Student's Prior Education Information. 
      </meta>
      
      <id name = "priorEducationId" type = "int" column = "PriorEducationId">
         <generator class="native"/>
      </id>
      
      <property name = "neuId" column = "NeuId" type = "string"/>    
      <property name = "institutionId" column = "InstitutionId" type = "string"/>
      <property name = "majorId" column = "MajorId" type = "string"/>
      <property name = "graduationDate" column = "GraduationDate" type = "string"/>
      <property name = "degreeLevel" column = "DegreeLevel" type = "string"/>
      <property name = "gpa" column = "Gpa" type = "string"/>
      <property name = "degreeCandidacy" column = "DegreeCandidacy" type = "string"/>
       
   </class>
</hibernate-mapping>