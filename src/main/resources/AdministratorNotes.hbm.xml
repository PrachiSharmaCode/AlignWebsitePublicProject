<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name = "org.mehaexample.asdDemo.model.AdministratorNotes" table="AdministratorNotes">

        <meta attribute = "class-description">
            This class contains the Company detail.
        </meta>
        <id name = "administratorNoteId" column="AdministratorNoteId" type="int">
            <generator class="native"/>
        </id>

        <property name = "title" column = "Title" type = "string" />
        <property name = "desc" column = "Description" type = "string" />

        <many-to-one name="student" class="org.mehaexample.asdDemo.model.Students"
                     column="NeuId" />

        <many-to-one name="admin" class="org.mehaexample.asdDemo.model.Administrators"
                     column="AdministratorNeuId" />

    </class>
</hibernate-mapping>